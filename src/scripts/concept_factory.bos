QueryNanoPiece(piecenum)
{
	if( nano_index == 1 )
	{
		piecenum = rfnano;
	}
	if( nano_index == 2 )
	{
		piecenum = lfnano;
	}
	if( nano_index == 3 )
	{
		piecenum = rbnano;
	}
	if( nano_index == 4 )
	{
		piecenum = lbnano;
	}
	++nano_index;
	if( nano_index > 4 )
	{
		nano_index = 1;
	}
	call-script lua_QueryNanoPiece(piecenum);
}

Activate()
{
	signal SIG_ACTIVATE;
	start-script RequestState(0);
}

Deactivate()
{
	signal SIG_ACTIVATE;
	set-signal-mask SIG_ACTIVATE;
	sleep 5000;
	set-signal-mask 0;
	start-script RequestState(1);
}

