	Static_Var_1 = 1;
}

StopMoving()
{
	Static_Var_1 = 0;
}

SweetSpot(piecenum)
{
	piecenum = chest;
}

QueryNanoPiece(Func_Var_1)
{
	Func_Var_1 = 2;
}

RestorePosition()
{
	Static_Var_4 = 0;
	turn chest to y-axis <0.000000> speed <90.000000>;
	wait-for-turn chest around y-axis;
	building = FALSE;
}

AimFromPrimary(Func_Var_1)
{
	Func_Var_1 = 1;
}

QueryPrimary(Func_Var_1)
{
	Func_Var_1 = 2;
}

AimPrimary(Func_Var_1, Func_Var_2)
{
	if( Static_Var_4 == 1 )
	{
		return (0);
	}
	signal 2;
	set-signal-mask 2;
	building = TRUE;
	while( !Static_Var_3 )
	{
		sleep 100;
	}
	turn chest to y-axis Func_Var_1 speed <300.000000>;
	turn l_sho to x-axis <0.000000> - Func_Var_2 - <30.000000> speed <90.000000>;
	wait-for-turn chest around y-axis;
	wait-for-turn l_sho around x-axis;
	fire = 1;
	return (1);
}

