TargetCleared(weaponnum)
{
	signal 2;
	set-signal-mask 2;
	call-script RestorePosition();
}

StopBuilding()
{
	set INBUILDSTANCE to 0;
	signal 2;
	set-signal-mask 2;
	call-script RestorePosition();
}

Killed(severity, corpsetype)
{
	hide bigflsh;
	hide nanospray;
	corpsetype = 1;
	explode chest type SHATTER | EXPLODE_ON_HIT | BITMAP2;
	explode chest type FALL | SMOKE | FIRE | EXPLODE_ON_HIT | BITMAP1;
	explode l_upleg type SHATTER | EXPLODE_ON_HIT | BITMAP2;
	explode l_upleg type FALL | SMOKE | FIRE | EXPLODE_ON_HIT | BITMAP2;
	explode l_lowleg type SHATTER | EXPLODE_ON_HIT | BITMAP2;
	explode l_lowleg type FALL | SMOKE | FIRE | EXPLODE_ON_HIT | BITMAP1;
	explode r_upleg type SHATTER | EXPLODE_ON_HIT | BITMAP2;
	explode r_upleg type FALL | SMOKE | FIRE | EXPLODE_ON_HIT | BITMAP2;
	explode r_lowleg type SHATTER | EXPLODE_ON_HIT | BITMAP2;
	explode r_lowleg type FALL | SMOKE | FIRE | EXPLODE_ON_HIT | BITMAP1;
	explode r_dgun type SHATTER | EXPLODE_ON_HIT | BITMAP2;
	explode r_dgun type FALL | SMOKE | FIRE | EXPLODE_ON_HIT | BITMAP1;
	explode nanospray type SHATTER | EXPLODE_ON_HIT | BITMAP2;
	explode nanospray type FALL | SMOKE | FIRE | EXPLODE_ON_HIT | BITMAP2;
	explode head type SHATTER | EXPLODE_ON_HIT | BITMAP2;
	explode head type FALL | SMOKE | FIRE | EXPLODE_ON_HIT | BITMAP1;
}

