	move Thigh1 to y-axis [0.000000] now;
	move Thigh2 to x-axis [0.000000] now;
	move Thigh2 to y-axis [0.000000] now;
	move Foot2 to x-axis [0.000000] now;
	move Leg1 to x-axis [0.000000] now;
	sleep 200;
}

